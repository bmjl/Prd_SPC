@model SPC.Models.Select

@{
	ViewBag.Title = "选择";
}

<div class="form-horizontal">
	@Html.ValidationSummary(true, "", new { @class = "text-danger" })
	<div class="form-group">
		@Html.LabelFor(model => model.DptName, htmlAttributes: new { @class = "control-label col-md-2" })
		<div class="col-md-10">
			<select id="dpt" class="form-control">
				<option value="0">请选择</option>
			</select>
		</div>
	</div>

	<div class="form-group">
		@Html.LabelFor(model => model.PrdName, htmlAttributes: new { @class = "control-label col-md-2" })
		<div class="col-md-10">
			<select id="prd" class="form-control">
				<option value="0">请选择</option>
			</select>
		</div>

	</div>

	<div class="form-group">
		@Html.LabelFor(model => model.PjtName, htmlAttributes: new { @class = "control-label col-md-2" })
		<div class="col-md-10">
			<select id="pjt" class="form-control">
				<option value="0">请选择</option>
			</select>
		</div>
	</div>

	<div class="form-group">
		@Html.LabelFor(model => model.SetName, htmlAttributes: new { @class = "control-label col-md-2" })
		<div class="col-md-10">
			<select id="set" class="dpt form-control">
				<option value="0">请选择</option>
			</select>

		</div>
	</div>

</div>